# GUL data manipulation
G['DateTime'] = pd.to_datetime(G['Date'] +' ' + G['Time'])
U['DateTime'] = pd.to_datetime(U['Date'] +' ' + U['Time'])
L['DateTime'] = pd.to_datetime(L['Date'] +' ' + L['Time'])
